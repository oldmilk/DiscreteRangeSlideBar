<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DiscreteRangeSlideBar">
        <attr name="rangeCount" format="integer" />
        <attr name="multiplier" format="integer" />
        <attr name="startIndex" format="integer" />
        <attr name="unit" format="string" />

        <attr name="filledSlotRadius" format="dimension" />
        <attr name="filledColor" format="color" />
        <attr name="filledTextColor" format="color" />
        <attr name="filledTextSize" format="dimension" />
        <attr name="emptySlotRadius" format="dimension" />
        <attr name="emptyColor" format="color" />
        <attr name="emptyTextColor" format="color" />
        <attr name="emptyTextSize" format="dimension" />
        <attr name="selectedSlotRadius" format="dimension" />
        <attr name="selectedColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="selectedTextSize" format="dimension" />
        <attr name="selectedShowShadow" format="boolean" />
        <attr name="barHeight" format="dimension" />

        <attr name="isDrawRuler" format="boolean" />


    </declare-styleable>
</resources>